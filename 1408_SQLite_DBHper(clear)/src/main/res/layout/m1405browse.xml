<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rel3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".M1405browse">

    <TextView
        android:id="@+id/tvIdTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@color/Yellow"
        android:text="@string/tvIdTitle"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/Navy"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/TextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/tv001"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvIdTitle"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        android:textColor="@color/Red"
        app:layout_constraintBaseline_toBaselineOf="@+id/TextView1"
        app:layout_constraintBottom_toBottomOf="@+id/TextView1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView1"
        tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck,TextContrastCheck"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/TextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/tv002"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edtGrp"
        app:layout_constraintBottom_toBottomOf="@+id/edtGrp"
        app:layout_constraintStart_toStartOf="@+id/TextView1"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtGrp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView2"
        app:layout_constraintTop_toBottomOf="@+id/edtName"
        tools:ignore="TouchTargetSizeCheck,TextContrastCheck"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/TextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/tv003"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/TextView2"
        app:layout_constraintTop_toBottomOf="@+id/TextView2"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtAddr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView3"
        app:layout_constraintTop_toBottomOf="@+id/edtGrp"
        tools:ignore="TouchTargetSizeCheck,TextContrastCheck"
        tools:targetApi="o" />


    <TextView
        android:id="@+id/count_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/count_t"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="TextContrastCheck" />


    <Button
        android:id="@+id/btIdtop"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdtop"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView3" />

    <Button
        android:id="@+id/btIdPrev"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdPrev"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btIdtop"
        app:layout_constraintBottom_toBottomOf="@+id/btIdtop"
        app:layout_constraintStart_toEndOf="@+id/btIdtop"
        tools:text="@string/btIdPrev" />

    <Button
        android:id="@+id/btIdNext"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdnext"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btIdPrev"
        app:layout_constraintBottom_toBottomOf="@+id/btIdPrev"
        app:layout_constraintStart_toEndOf="@+id/btIdPrev" />

    <Button
        android:id="@+id/btIdend"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdend"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/btIdNext"
        app:layout_constraintTop_toTopOf="@+id/btIdNext" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/moveico" />
</androidx.constraintlayout.widget.ConstraintLayout>
