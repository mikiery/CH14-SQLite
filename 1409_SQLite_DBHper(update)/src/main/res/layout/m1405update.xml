<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rel3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:padding="@dimen/Spacing">

    <TextView
        android:id="@+id/tvIdTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tvIdTitle"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/Navy"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:background="@color/Teal"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/txtid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:hint="@string/hint"
        android:text="@string/txtid"
        android:textColor="@color/Red"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvIdTitle"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/White"
        android:ems="10"
        android:enabled="false"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        android:textColor="@color/Red"
        app:layout_constraintBottom_toBottomOf="@+id/txtid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/edtName"
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck,DuplicateSpeakableTextCheck"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/TextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/tv001"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edid"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/TextView1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView1"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,TouchTargetSizeCheck"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/TextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/tv002"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtName"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtGrp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView2"
        app:layout_constraintTop_toBottomOf="@+id/edtName"
        tools:ignore="TouchTargetSizeCheck,TextContrastCheck"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/TextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/tv003"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView2"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/edtAddr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/White"
        android:hint="@string/hint"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TextView3"
        app:layout_constraintTop_toBottomOf="@+id/edtGrp"
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck"
        tools:targetApi="o" />


    <TextView
        android:id="@+id/count_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/count_t"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/btIdtop"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdtop"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextView3" />

    <Button
        android:id="@+id/btIdPrev"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdPrev"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btIdtop"
        app:layout_constraintBottom_toBottomOf="@+id/btIdtop"
        app:layout_constraintStart_toEndOf="@+id/btIdtop"
        tools:text="@string/btIdPrev" />

    <Button
        android:id="@+id/btIdNext"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdnext"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btIdPrev"
        app:layout_constraintBottom_toBottomOf="@+id/btIdPrev"
        app:layout_constraintStart_toEndOf="@+id/btIdPrev" />

    <Button
        android:id="@+id/btIdend"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_default"
        android:text="@string/btIdend"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/btIdNext"
        app:layout_constraintTop_toTopOf="@+id/btIdNext" />

    <Button
        android:id="@+id/btnupdate"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:background="@android:drawable/btn_default"
        android:text="@string/btnupdate"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/btIdtop"
        app:layout_constraintTop_toBottomOf="@+id/btIdtop" />

    <Button
        android:id="@+id/btIdDel"
        android:layout_width="@dimen/Buttonsize"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@android:drawable/btn_default"
        android:text="@string/btndel"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnupdate"
        app:layout_constraintStart_toEndOf="@+id/btIdPrev"
        app:layout_constraintTop_toBottomOf="@+id/btIdPrev" />



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/pic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/moveico" />
</androidx.constraintlayout.widget.ConstraintLayout>
